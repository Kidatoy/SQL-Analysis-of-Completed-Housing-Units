SELECT *
  FROM tutorial.us_housing_units
    (will provide the full data through Mode.com)
    
    SELECT year,
       month,
       west
  FROM tutorial.us_housing_units
    (view of columns year, month, and west)
    
    SELECT west AS "West Region"
  FROM tutorial.us_housing_units
    (changing the column title of west to "West Region")
    
    SELECT *
  FROM tutorial.us_housing_units
 LIMIT 100
  (limiting the results of the query)
  
  SELECT *
  FROM tutorial.us_housing_units
 WHERE month = 1
  (isolating individual month results)
  
  SELECT *
  FROM tutorial.us_housing_units
 WHERE west > 30
  (limiting results of the West region column to > 30 units)
  
  SELECT *
  FROM tutorial.us_housing_units
 WHERE month_name != 'January'
  (Shows the other months other than January)
  
  SELECT year,
       month,
       west,
       south,
       west + south AS south_plus_west
  FROM tutorial.us_housing_units
  (combining columns and renaming the new group)
  

  
    
    
